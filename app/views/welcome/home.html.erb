<a href='/auth/facebook'>Login with Facebook</a>
<% if session[:user_id] %>
    <h1> User name: <%= @user.name %>
    <h2> Email:<%= @user.email %></h2>
    <h2> Facebook UID: <%= @user.uid %></h2>
    <img src="<%= @user.image %>">
<% else %>
    <%= link_to('Log in with Facebook!', '/auth/facebook') %>
<% end %>
